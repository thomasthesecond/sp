{{ render "@banner" }}

<main id="main" role="main" tabindex="0">
  {{> @masthead className="js-masthead" animate=true }}

  <div class="sticky">
    {{ render "@location-finder" }}
  </div>

  <div class="LocationsPage-grid container grid">
    <aside class="LocationsPage-aside">
      {{> @location-map }}
    </aside>

    <div class="LocationsPage-content">
      {{> @location-list }}
    </div>
  </div>
</main>

{{ render "@contentinfo" }}
