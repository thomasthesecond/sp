{
  "parser": "babel-eslint",
  "globals": {
    "$": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "arrow-parens": [0],
    "quotes": [1, "double"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "strict": [0, "never"],
    "no-var": [1],
    "no-unused-vars": [2],
    "semi": [1, "always"],
    "no-use-before-define": [2],
    "no-undef": [2],
    "no-param-reassign": [0]
  },
  "extends": ["airbnb-base"]
}
